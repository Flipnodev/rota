{
  "name": "rota",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "mobile": "pnpm --filter @rota/mobile start",
    "mobile:ios": "pnpm --filter @rota/mobile ios",
    "mobile:android": "pnpm --filter @rota/mobile android",
    "web": "pnpm --filter @rota/web dev"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  }
}
